{
  "name": "nodemeet",
  "version": "1.0.0",
  "description": "social network app",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "nodemon": "nodemon --config nodemon.json src/app.ts",
    "build": "tsc",
    "tailwind": "postcss src/public/css/tailwind.css -o src/public/css/styles.css --watch",
    "start": "concurrently \"npm run nodemon\" \"npm run tailwind\""
  },
  "author": "Jakub Jurkian",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "autoprefixer": "^10.4.19",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.1",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.3",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express-session": "^1.18.0",
    "tailwindcss": "^3.4.1"
  }
}
